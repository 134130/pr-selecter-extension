{
  "manifest_version": 3,
  "name": "PR selector",
  "description": "Select PR templates by fixed file names (FEATURE.md, FIX.md)",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/compare/*"],
      "css": ["styles.css"],
      "js": ["script.js"],
      "run_at": "document_start"
    }
  ]
}
